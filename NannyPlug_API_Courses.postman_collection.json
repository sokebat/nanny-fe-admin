          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/public/courses?page=1&limit=10&category=&search=&minPrice=&maxPrice=",
              "host": ["{{base_url}}"],
              "path": ["public", "courses"],
              "query": [
                {
                  "key": "page",
                  "value": "1",
                  "description": "Page number (default: 1)",
                  "disabled": false
                },
                {
                  "key": "limit",
                  "value": "10",
                  "description": "Items per page (default: 10)",
                  "disabled": false
                },
                {
                  "key": "category",
                  "value": "",
                  "description": "Filter by category",
                  "disabled": true
                },
                {
                  "key": "search",
                  "value": "",
                  "description": "Search in title/description",
                  "disabled": true
                },
                {
                  "key": "minPrice",
                  "value": "",
                  "description": "Minimum price filter",
                  "disabled": true
                },
                {
                  "key": "maxPrice",
                  "value": "",
                  "description": "Maximum price filter",
                  "disabled": true
                }
              ]
            },
            "description": "Get list of all listed courses. No authentication required.\n\n**Query Parameters:**\n- `page`: Page number (default: 1)\n- `limit`: Items per page (default: 10)\n- `category`: Filter by category (optional)\n- `search`: Search in title/description (optional)\n- `minPrice`: Minimum price (optional)\n- `maxPrice`: Maximum price (optional)"
          },
          "response": []
        },
        {
          "name": "Get Popular Courses",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/public/courses/popular?limit=10",
