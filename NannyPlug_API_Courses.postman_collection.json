              "path": ["public", "courses", "popular"],
              "query": [
                {
                  "key": "limit",
                  "value": "10",
                  "description": "Number of popular courses to return (default: 10)",
                  "disabled": false
                }
              ]
            },
            "description": "Get list of popular/featured courses. No authentication required.\n\n**Query Parameters:**\n- `limit`: Number of courses to return (default: 10)"
          },
          "response": []
        },
        {
          "name": "Get Course Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/public/courses/{{course_id}}",
              "host": ["{{base_url}}"],
              "path": ["public", "courses", "{{course_id}}"]
